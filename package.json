{
  "name": "nodejs-spa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "docker:start:dev": "docker-compose up app -d --build",
    "docker:stop:dev": "docker-compose stop app",
    "docker:down:dev": "docker-compose down app",
    "docker:test": "docker-compose up test -d --build",
    "docker:e2e-test": "docker-compose up e2e-test -d --build",
    "commit": "git cz"
  },
  "dependencies": {
    "@auth0/auth0-react": "2.2.1",
    "@heroicons/react": "2.0.18",
    "@ibm-watson/assistant-web-chat-react": "1.1.0",
    "@sentry/react": "7.104.0",
    "@tanstack/react-query": "4.35.3",
    "@uppy/core": "3.7.1",
    "@uppy/dashboard": "3.7.1",
    "@uppy/image-editor": "2.1.2",
    "@uppy/drag-drop": "3.0.3",
    "@uppy/file-input": "3.0.4",
    "@uppy/progress-bar": "3.0.4",
    "@uppy/react": "3.2.1",
    "@uppy/webcam": "3.3.4",
    "autoprefixer": "10.4.15",
    "aws-amplify": "6.0.3",
    "chart.js": "4.4.0",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "dom-to-image": "2.6.0",
    "dotenv": "^16.3.1",
    "file-saver": "2.0.5",
    "flowbite-react": "0.5.0",
    "i18next": "23.4.9",
    "i18next-browser-languagedetector": "7.1.0",
    "jotai": "2.6.0",
    "jotai-effect": "0.2.3",
    "postcss": "8.4.28",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-i18next": "13.2.2",
    "react-markdown": "9.0.0",
    "react-player": "2.13.0",
    "react-router-dom": "6.15.0",
    "rehype-raw": "7.0.0",
    "swiper": "8.3.2",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@playwright/test": "^1.38.1",
    "@sentry/types": "^7.104.0",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/react-vite": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/dom-to-image": "^2.6.7",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "adm-zip": "^0.5.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "happy-dom": "^13.3.8",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.2.5",
    "playwright": "^1.38.1",
    "playwright-msw": "^2.2.1",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "resize-observer-polyfill": "1.5.1",
    "storybook": "^7.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-mkcert": "^1.16.0",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.34.3",
    "vitest-axe": "^0.1.0",
    "winston": "^3.11.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix --max-warnings 0 --report-unused-disable-directives"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "scopes": [
        "analytics",
        "auth",
        "api",
        "ci",
        "components",
        "data",
        "deps",
        "e2e-test",
        "infra",
        "storybook",
        "tooling",
        "ui",
        "unit-test",
        "utils"
      ],
      "defaultScope": "",
      "jiraPrefix": "VITA",
      "jiraPrepend": "[",
      "jiraAppend": "]"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
